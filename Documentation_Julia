    "function operaciones(a, b)\n",
    "    suma = a + b\n",
    "    resta = a - b\n",
    "    return suma, resta\n",
    "end\n",
    "\n",
    "res1, res2 = operaciones(10, 5)\n",
    "println(res1)  \n",
    "println(res2)  "
   ]
